# {{ ansible_managed }}
set default=0
set timeout=-1

{% for blueprint in edge_compose_blueprint_images %}
menuentry "Install image: {{ blueprint['name'] }}" {
  linuxefi vmlinuz root=live:{{ edge_compose_blueprint_repository_baseurl }}/install-{{ blueprint['distro'] }}.img inst.ks={{ edge_compose_blueprint_repository_baseurl }}/{{ blueprint['name'] }}/install.ks
  initrdefi initrd.img
}
{% endfor %}
