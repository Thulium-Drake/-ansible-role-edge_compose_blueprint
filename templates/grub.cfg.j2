# {{ ansible_managed }}
set default=0
set timeout=-1

{% for blueprint in edge_compose_blueprint_images %}
menuentry "Install image: {{ blueprint['name'] }}" {
  linuxefi vmlinuz root=live:http://{{ edge_compose_blueprint_deployment_server }}/install.img inst.ks=http://{{ edge_compose_blueprint_deployment_server }}/{{ blueprint['name'] }}/install.ks
  initrdefi initrd.img
}
{% endfor %}
